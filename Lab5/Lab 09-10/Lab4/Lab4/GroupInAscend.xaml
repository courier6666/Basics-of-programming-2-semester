<Window x:Class="Lab4.GroupInAscend"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab4"
        mc:Ignorable="d"
        Title="GroupInAscend" Height="262" Width="522">
    <Grid>
        <TextBox x:Name ="IDGroupBox" HorizontalAlignment="Left" Margin="86,91,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" Height="26"/>
        <TextBox x:Name ="IDAscendBox" HorizontalAlignment="Left" Margin="250,91,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" Height="26" />
        <Label Content="Id Group" HorizontalAlignment="Left" Margin="86,63,0,0" VerticalAlignment="Top" Width="75"/>
        <Label Content="Id Ascend" HorizontalAlignment="Left" Margin="250,63,0,0" VerticalAlignment="Top" Width="68"/>
        <Button x:Name ="AddButton" Content="Add" HorizontalAlignment="Left" Margin="86,122,0,0" VerticalAlignment="Top" Height="22" Width="68" Click="AddButton_Click"/>
        <Button x:Name ="DeleteButton" Content="Delete" HorizontalAlignment="Left" Margin="212,122,0,0" VerticalAlignment="Top" Height="22" Width="68" Click="DeleteButton_Click"/>
        <Button x:Name ="ExitButton" Content="Cancel" HorizontalAlignment="Left" Margin="332,122,0,0" VerticalAlignment="Top" Height="22" Width="68" Click="ExitButton_Click"/>

    </Grid>
</Window>
